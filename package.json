{
  "name": "aarohans-142",
  "version": "2.0.0",
  "description": "Cypress cucumber framework",
  "main": "index.js",
  "scripts": {
    "script": "npx cypress run --browser edge --config-file cypress.config.js",
    "postTest": "node cucumber-html-report.js",
    "test":"npm run script && npm run postTest"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^12.17.4",
    "cypress-cucumber-preprocessor": "^4.3.1"
  },
  "cypress-cucumber-preprocessor": {

      "nonGlobalStepDefinitions": true,
      "step_definitions": "cypress/e2e",
        "cucumberJson": {
          "generate": true,
         "outputFolder": "cypress/reports/cucumber-json",
          "filePrefix": "",
          "fileSuffix": ".cucumber"
    }
     
    } ,
    "dependencies": {
      "cucumber": "^6.0.7",
      "multiple-cucumber-html-reporter": "^3.4.0",
      "cypress-real-events": "^1.10.1"
    }
}
